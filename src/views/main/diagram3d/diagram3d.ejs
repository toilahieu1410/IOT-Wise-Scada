<style>
  .main-panel1 {
    -webkit-transition: width 0.25s ease, margin 0.25s ease;
    transition: width 0.25s ease, margin 0.25s ease;
    width: 100%;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -ms-flex-direction: column;
    flex-direction: column;
}
.title {
    margin: 10px;
    color:#fff;
  }
  #view {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  cursor: move;
}
.list-station {
  justify-items: center;
}
.pump-station {
  text-align: center;
  min-width: 150px;
  border-radius: 5px;
  padding: 5px;
  color:#fff;
  background-color: #21967ca1;
  position: absolute;
  top:-15px;
  left: 15px;
}
.dashboard {
  background-color: rgb(99 158 143 / 6%);
    box-shadow: 0 5px 30px rgb(0 0 0 / 0%), 0 11px 10px rgba(0,0,0,0.22);
    border-radius: 5px;
    transition: all 0.3s;
    margin-bottom:40px;
    min-width: 355px;
    max-height: 250px;
}
#board {
  position: relative;
}
.color_1 {
  background:transparent;
  &:hover,
  &.box_full {
    background: darken(#524656, 10%);
  }
}

.color_2 {
  background: #cf4647;
  &:hover,
  &.box_full {
    background: darken(#cf4647, 10%);
  }
}

.color_3 {
  background: #a7c5bd;
  &:hover,
  &.box_full {
    background: darken(#a7c5bd, 10%);
  }
}

.color_4 {
  background: #e5ddcb;
  &:hover,
  &.box_full {
    background: darken(#e5ddcb, 10%);
  }
}
.box-diagram {
  transition: all 0.3s;
    width: 18%;
    perspective-origin: 25% 50%;
    height: 150px;
    position: relative;
    cursor: pointer;
    transform: rotate3d(-9,-5,7,-70deg);
}
.box-diagram .center {
  display: flex;
  align-items: center;
}
.center {
  height: 100%;
  font-weight: bold;
  font-size: 2em;
  color: white;
}
.box-diagram a:hover {
  text-decoration: none;
  color:#fff;
}
.box-diagram:hover {

  transition: .3s;
  background:transparent;
  /* box-shadow: -1px 1px 35px 5px rgba(0, 0, 0, 0.75); */
  z-index: 999;
}
