}

a,
fa {
  color: #fefeeb;
  &:hover {
    color: darken(#fefeeb, 15%);
  }
}
.is_hidden {
  opacity: 0;
  visibility: hidden;
}
.is_visible {
  visibility: visible;
  opacity: 1;
}
.is_visible,
.is_hidden {
  -webkit-transition: opacity 0.5s ease;
  -moz-transition: opacity 0.5s ease;
  -o-transition: opacity 0.5s ease;
  transition: opacity 0.5s ease;
}

.center {
  position: absolute;
  margin-left: auto;
  margin-right: auto;
  left: 0;
  right: 0;
}


.fade_me {
  transform: scale(0);
  transition: transform 1.5s ease;
}


.slide {
