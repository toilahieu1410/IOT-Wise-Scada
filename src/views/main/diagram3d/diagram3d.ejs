.pos_relative {
  position: relative;
}

.top-right {
  position: absolute;
  top: 5px;
  right: 5px;
}

.box_full .center {
  display: none;
}
.box_1 {
  display: inline-block;
    bottom: -253px;
    left: 38%;
}

.box_2 {
  top: 0;
  left: 50%;
}

.box_3 {
  top: 50%;
  left: 0;
}

.box_4 {
  top: 50%;
  left: 50%;
}

a,
fa {
  color: #fefeeb;
  &:hover {
    color: darken(#fefeeb, 15%);
  }
}
.is_hidden {
  opacity: 0;
  visibility: hidden;
}
.is_visible {
  visibility: visible;
  opacity: 1;
}
.is_visible,
.is_hidden {
  -webkit-transition: opacity 0.5s ease;
  -moz-transition: opacity 0.5s ease;
  -o-transition: opacity 0.5s ease;
  transition: opacity 0.5s ease;
}

.center {
  position: absolute;
  margin-left: auto;
  margin-right: auto;
  left: 0;
  right: 0;
}


.fade_me {
  transform: scale(0);
  transition: transform 1.5s ease;
}


.slide {
  text-align: center;
}

</style>

<script>
  $box1 = $('box_1');
$box4 = $('box_2');
$box4 = $('box_3');
$box4 = $('box_4');

$('.box-diagram').click(function() {
  $(this).addClass('box_full');
     //$(this).addClass('fade_me');

  $(this).siblings().addClass("fade_me");
  setTimeout(function() {
 $(".is_hidden").addClass("is_visible").removeClass("is_hidden");
  }, 500);
});
$('a').click(function() {
  event.stopPropagation();
  $(".is_visible").addClass("is_hidden").removeClass("is_visible");
  $(this).closest('.box-diagram').removeClass('box_full');
  $(".box-diagram").not(this).removeClass("fade_me");
});

$(document).ready(function() {
    $('#fullpage').fullpage();
});
</script>
